@model MetaTags
@inherits StatiqRazorPage<MetaTags>

@* <meta name="twitter:label1" content="Written by" />
<meta name="twitter:data1" content="@Model.Author" />
<meta name="twitter:title" content="@Model.PageTitle" />
<meta property="twitter:image" content="@Model.PostImageUrl" />
