---
ShowInNavbar: false
---
<!-- data.cshtml -->

<table>
  <tr>
    <th>Name</th>
    <th>AvatarPath</th>
    <th>Bio</th>
    <th>CodingGame</th>
    <th>Discord</th>
    <th>Facebook</th>
    <th>LinkedIn</th>
    <th>Twitter</th>
  </tr>
  @foreach (var author in Authors.Values)
  {
    <tr>
      <td>@author.Name</td>
      <td>@author.AvatarPath</td>
      <td>@author.Bio</td>
      <td>@author.CodingGame</td>
      <td>@author.Discord</td>
      <td>@author.Facebook</td>
      <td>@author.LinkedIn</td>
      <td>@author.Twitter</td>
    </tr>
  }
</table>


